{
    "name" : "makinacorpus/goat-hydrator",
    "type": "library",
    "description": "Advanced configurable flat and hierarchical object hydrator",
    "keywords": ["orm", "database", "symfony", "hydrator"],
    "homepage": "http://github.com/makinacorpus/php-hydrator",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name" : "Pierre RINEAU",
            "email" : "pierre.rineau@processus.org"
        }
    ],
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "autoload" : {
        "psr-4" : {
            "Goat\\Hydrator\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goat\\Hydrator\\Tests\\": "tests"
        }
    },
    "require" : {
        "makinacorpus/generated-hydrator": "*",
        "php" : "^5.6|^7.0"
    },
    "require-dev" : {
        "phpbench/phpbench": "^0.13.0",
        "phpunit/phpunit" : "5.*",
        "symfony/config": "^3.3|^4.1|^5.0",
        "symfony/dependency-injection": "^3.3|^4.0|^5.0",
        "symfony/http-kernel": "^3.3|^4.0|^5.0",
        "symfony/property-info": "^3.3|^4.0|^5.0",
        "symfony/validator": "^3.3|^4.0|^5.0",
        "symfony/yaml": "^4.1|^5.0"
    },
    "suggest" : {
        "symfony/property-info" : "enables object tree hydration using @var doc blocks",
        "symfony/validator" : "enables mapper entity validation"
    }
}
